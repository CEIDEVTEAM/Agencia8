{
  "name": "Agencia8",
  "version": "0.4.0",  
  "description": "Desarrollo para agencia.",
  "private": true,
  "scripts": {
    "tailwind:dev": "tailwindcss build src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "tailwind:build": "cross-env NODE_ENV=production postcss src/assets/css/tailwind.css -o src/assets/css/tailwind.output.css",
    "prestart": "npm run tailwind:dev",
    "start": "react-scripts --openssl-legacy-provider start",
    "prebuild": "npm run tailwind:build",
    "build": "react-app-rewired build",
    "cz": "git-cz",
    "release": "release-it",
    "test": "react-app-rewired test",
    "eject": "react-scripts eject"
  },
  "dependencies": {
    "@react-leaflet/core": "1.0.2",
    "@testing-library/jest-dom": "4.2.4",
    "@testing-library/react": "9.5.0",
    "@testing-library/user-event": "7.2.1",
    "@windmill/react-ui": "0.3.1",
    "axios": "^0.27.2",
    "chart.js": "2.9.3",
    "classnames": "2.2.6",
    "faker": "4.1.0",
    "formik": "^2.2.9",
    "leaflet": "1.7.1",
    "powerbi-client": "^2.21.1",
    "powerbi-client-react": "^1.3.5",
    "react": "^16.13.1",
    "react-app-rewired": "^2.2.1",
    "react-chartjs-2": "2.9.0",
    "react-data-table-component": "^7.5.2",
    "react-datepicker": "^4.8.0",
    "react-dom": "^16.13.1",
    "react-focus-lock": "2.4.0",
    "react-leaflet": "3.1.0",
    "react-router-dom": "5.2.0",
    "react-scripts": "3.4.1",
    "react-toastify": "^9.0.0",
    "react-transition-group": "4.4.1",
    "sweetalert2": "^11.4.29",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "1.1.4",
    "@svgr/webpack": "5.4.0",
    "autoprefixer": "9.8.0",
    "commitizen": "4.1.2",
    "cross-env": "7.0.2",
    "cssnano": "4.1.10",
    "cz-conventional-changelog": "3.2.0",
    "postcss-cli": "7.1.1",
    "react-axe": "3.5.2",
    "release-it": "13.6.4",
    "tailwindcss": "1.4.6"
  },
  "keywords": [
    "admin",
    "tailwind",
    "react"
  ],
  "release-it": {
    "github": {
      "release": true
    },
    "npm": {
      "publish": false
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular",
        "infile": "CHANGELOG.md"
      }
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
